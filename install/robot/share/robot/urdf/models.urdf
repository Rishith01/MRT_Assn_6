</joint>

<joint name="fixed_joint_hand2" type="fixed">
    <parent>arm_hand_2</parent>
    <child>arm_hand_3_ext</child>
</joint>

<joint name="front_depth_camera_joint" type="fixed">
    <parent>chassis</parent>
