<joint name="arm_base_joint" type="revolute">
  <parent link="chassis"/>
  <child link="arm_base"/>
  <origin xyz="0.4 0 0.435" rpy="0 0 0"/>
  <axis xyz="0 0 1"/>
  <limit lower="-3.1416" upper="3.1416"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>

<joint name="arm_joint_2" type="revolute">
  <parent link="arm_base"/>
